<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="gold_journal" model="account.journal">
            <field name="name">Gold</field>
            <field name="code">GOLD</field>
            <field name="type">general</field>
            <field name="gold" eval="True"/>
            <field name="show_on_dashboard" eval="False"/>
            <field name="active">False</field>
        </record>

        <record id="make_value_charg" model="making.charges">
            <field name="name">make value</field>
        </record>

        <record id="make_value_product" model="product.product">
            <field name="name">make value</field>
            <field name="list_price">0.0</field>
            <field name="standard_price">0.0</field>
            <field name="type">service</field>
            <field name="default_code">MAKE_VALUE</field>
            <field name="making_charge_id" ref="make_value_charg"/>
        </record>

    </data>
</odoo>